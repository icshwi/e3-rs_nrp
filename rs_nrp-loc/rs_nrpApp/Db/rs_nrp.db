############ CSS X/Y Display ############
########## --> Time Array <-- ##########
record(acalcout, "$(P)$(R)timeArray_")
{
    field(SCAN, "Passive")
    field(PINI, "YES")
    field(NELM, "5000")
    field(NUSE, "260")
    field(INPA, "$(P)$(R)timeArray_.NUSE")
    field(INPB, "$(P)$(R)Trace_time_callback CP")
    field(INPC, "$(P)$(R)Trace_offset_time_callback CP")
    field(CALC, "IX*(@1*1000/@0)+((IX+1)/(IX+1))*@2*1000")
    #field(OUT,  "$(P)$(R)timeArray PP")
    field(FLNK, "$(P)$(R)timeArrayCALC")
    field(EGU,  "ms")
}
record(acalcout, "$(P)$(R)timeArray_us")
{
    field(SCAN, "Passive")
    field(PINI, "YES")
    field(NELM, "5000")
    field(NUSE, "260")
    field(INPA, "$(P)$(R)timeArray_us.NUSE")
    field(INPB, "$(P)$(R)Trace_time_callback CP")
    field(INPC, "$(P)$(R)Trace_offset_time_callback CP")
    field(CALC, "1000*(IX*(@1*1000/@0)+((IX+1)/(IX+1))*@2*1000)")
    #field(OUT,  "$(P)$(R)timeArray PP")
    field(FLNK, "$(P)$(R)timeArrayCALC")
    field(EGU,  "us")
}
record(acalcout, "$(P)$(R)timeArrayCALC")
{
    field(SCAN, "Passive")
    field(NELM, "5000")
    field(NUSE, "260")
    field(INPA, "$(P)$(R)Trace_time_callback CP")
    field(INAA, "$(P)$(R)timeArray_.AVAL PP")
    field(INBB, "$(P)$(R)timeArray_us.AVAL PP")
    field(CALC, "@0<0.003?@@1:@@0")
    field(OUT,  "$(P)$(R)timeArray PP")
}
record(waveform, "$(P)$(R)timeArray")
{
    field(DESC, "Time X-axis: ms/us unit")
    field(SCAN, "1 second")
    field(FTVL, "DOUBLE")
    field(NELM, "5000")
    field(EGU,  "ms/us")
    field(PREC, "3")
}
