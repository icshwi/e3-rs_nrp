record(calcout, "$(P)$(R)Trace_level_W")
{
    field(SCAN, "Passive")
    field(DTYP, "stream")
    field(OUT,  "@rs_nrp.proto setTracelevel $(PORT)")
    field(CALC, "(10^(A/10))/1000")
    field(INPA, "$(P)$(R)Trace_level_DBM.VAL")
    field(OOPT, "Every Time")
    field(DOPT, "Use CALC")
    field(PREC, "3")
}
record(ai,"$(P)$(R)Trace_level_callback_W")
{
    field(SCAN, "1 second")
    field(DTYP, "stream")
