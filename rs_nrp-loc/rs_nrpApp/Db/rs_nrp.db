########## Get Power-Sensor Manufactor ##########
record(stringin, "$(P)$(R)GetManufactor")
{
    field(DTYP, "stream")
    field(INP,  "@rs_nrp.proto getPOWmanufactor $(PORT)")
    field(SCAN, "1 second")
}
########## Reset ##########
record(bo, "$(P)$(R)reset")
{
    field(SCAN, "Passive")    
    field(DTYP, "stream")
    field(OUT,  "@rs_nrp.proto Reset $(PORT)")
    field(VAL,  "0")
    field(PINI, "YES")
    field(ZNAM, "reset off")
    field(ONAM, "reset on")
}
########## SetRun for Power Sensor ##########
record(bo, "$(P)$(R)setRun")
{
    field(SCAN, "Passive")    
    field(DTYP, "stream")
    field(OUT,  "@rs_nrp.proto setRun $(PORT)")
    field(VAL,  "0")
    field(PINI, "YES")
    field(ZNAM, "run off")
    field(ONAM, "run on")
}
########## <--Main GUI Parameter--> ###########
#######################################
########## Frequency setting ##########
record(ao,"$(P)$(R)FREQ_value")
{
    field(SCAN, "1 second")
    field(PHAS, "0")
    field(FLNK, "$(P)$(R)setFREQ")
    field(VAL,  "702.059")
    field(EGU,  "MHz")
    field(PREC, "3")
    field(PINI, "YES")
}
record(calcout,"$(P)$(R)setFREQ")
{
    field(SCAN, "1 second")
    field(PHAS, "1")
    field(DTYP, "stream")
    field(OUT,  "@rs_nrp.proto setFrequency $(PORT)")
    field(CALC, "A*B")
    field(INPA, "$(P)$(R)FREQ_value.VAL")
    field(INPB, "1e6")
    field(OOPT, "Every Time")
    field(DOPT, "Use CALC")
}

record(ai, "$(P)$(R)getFREQ")
{
    field(SCAN, "1 second")
    field(DTYP, "stream")
    field(INP,  "@rs_nrp.proto getFrequency $(PORT)")
    field(EGU,  "Hz")
    field(PREC, "3")
}
