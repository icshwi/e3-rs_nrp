    field(EGU,  "W")
    field(PREC, "4")
    field(FLNK, "$(P)$(R)Trace_level_callback_DBM")
}
record(calc, "$(P)$(R)Trace_level_callback_DBM")
{
    field(SCAN, "Passive")
    field(INPA, "$(P)$(R)Trace_level_callback_W.VAL PP")
    field(CALC, "10*LOG(A)+30")
    field(EGU,  "dBm")
    field(PREC, "2")
    field(HIHI, "20")
    field(HIGH, "18")
    field(LOW,  "-36")
    field(LOLO, "-40")
    field(HHSV, "MAJOR")
    field(HSV,  "MINOR")
    field(LSV,  "MINOR")
    field(LLSV, "MAJOR")
}
